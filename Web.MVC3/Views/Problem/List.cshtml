@model IEnumerable<Data.ViewModels.Problem.ProblemIndexItemModel>
@using Data.Enums
@using Framework.Lists
@using Framework.Strings
@using Globalization.Resources.Problem
@using Web.Helpers
@if (Model.Count() == 0)
{
    @Resource.NoProblems
}
else
{
    foreach (var problem in Model.AsStyleableList())
    {
    <div style="margin-bottom: 32px;">
        @Html.Partial(MVC.Problem.Views.ListItem, problem.Item)
    </div>
    }
}